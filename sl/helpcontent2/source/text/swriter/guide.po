msgctxt ""
"fields.xhp\n"
"par_id3155582\n"
"help.text"
msgid "Field Type"
msgstr "Vrsta polja"

#. AywKe
#: fields.xhp
msgctxt ""
"fields.xhp\n"
"par_id3147760\n"
"help.text"
msgid "Property"
msgstr "Lastnost"

#. mAMBB
#: fields.xhp
msgctxt ""
